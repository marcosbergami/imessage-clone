{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"prettier",
		"airbnb"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": "error",
		"quotes": "off",
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
	}
}